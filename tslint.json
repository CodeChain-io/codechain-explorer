{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "no-unused-variable": [true, {"ignore-pattern": "^_"}]
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}
